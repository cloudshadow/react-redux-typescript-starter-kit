{
  "name": "react-redux-typescript-starter-kit",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "start:dev": "webpack serve --hot --color --config ./webpack.config.dev.js",
    "start:prod": "npm run clean-dist && webpack --config ./webpack.config.prod.js --color",
    "start:test": "npm run clean-dist-stable && webpack --config ./webpack.config.test.js",
    "eslint": "./node_modules/.bin/eslint ./src/**/*.{ts,tsx}",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "clean-dist-stable": "npm run remove-dist-stable && mkdir dist-stable",
    "remove-dist": "rimraf ./dist",
    "remove-dist-stable": "rimraf ./dist-stable",
    "generate": "babel-node tools/generateTemplate/generate.js",
    "ungenerate": "babel-node tools/generateTemplate/ungenerate.js",
    "test": "jest --no-cache --runInBand --passWithNoTests",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/runtime": "7.16.3",
    "@reduxjs/toolkit": "^1.9.0",
    "@types/history": "5.0.0",
    "@types/jest": "27.0.3",
    "@types/jwt-decode": "3.1.0",
    "@types/node": "16.11.8",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "@types/react-redux": "7.1.24",
    "@types/react-router": "5.1.19",
    "axios": "1.1.3",
    "history": "5.3.0",
    "jwt-decode": "3.1.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.5",
    "react-router": "6.4.3",
    "react-router-dom": "^6.4.3",
    "redux": "4.2.0",
    "redux-observable": "^2.0.0",
    "redux-thunk": "2.4.0",
    "rxjs": "7.4.0",
    "typesafe-actions": "5.1.0",
    "typescript": "4.5.2"
  },
  "devDependencies": {
    "@babel/cli": "7.16.0",
    "@babel/core": "7.16.0",
    "@babel/node": "7.16.0",
    "@babel/plugin-transform-runtime": "7.16.4",
    "@babel/preset-env": "7.16.4",
    "@babel/preset-react": "7.16.0",
    "@babel/preset-typescript": "7.16.0",
    "@types/webpack-env": "1.16.3",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "babel-loader": "8.2.3",
    "bundle-loader": "0.5.6",
    "cross-env": "7.0.3",
    "css-loader": "6.5.1",
    "eslint": "8.2.0",
    "eslint-plugin-react": "7.27.0",
    "file-loader": "6.2.0",
    "happypack": "5.0.1",
    "html-webpack-plugin": "5.5.0",
    "husky": "7.0.4",
    "immer": "9.0.6",
    "jest": "27.3.1",
    "json-loader": "0.5.7",
    "lint-staged": "12.0.3",
    "mini-css-extract-plugin": "2.4.5",
    "postcss-loader": "6.2.0",
    "prettier": "2.4.1",
    "sass": "^1.43.4",
    "sass-loader": "12.3.0",
    "speed-measure-webpack-plugin": "1.5.0",
    "style-loader": "3.3.1",
    "terser-webpack-plugin": "5.2.5",
    "ts-loader": "9.2.6",
    "url-loader": "4.1.1",
    "webpack": "5.64.1",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "^4.5.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run eslint",
      "prettier --write",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    "defaults"
  ]
}
